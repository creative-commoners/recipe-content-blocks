{
    "name": "silverstripe/recipe-content-blocks",
    "type": "silverstripe-recipe",
    "description": "Add content blocks to your SilverStripe project",
    "homepage": "https://www.silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "^1.2",
        "silverstripe/versioned-admin": "^1.0",
        "dnadesign/silverstripe-elemental": "3.0.x-dev",
        "silverstripe/elemental-fileblock": "1.0.x-dev",
        "silverstripe/elemental-bannerblock": "1.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "suggest": {
        "dnadesign/silverstripe-elemental-subsites": "Add integration logic for Elemental and Subsites",
        "dnadesign/silverstripe-elemental-userforms": "Add integration logic for Elemental and Userforms"
    },
    "extra": {
        "project-files": [
            "app/*"
        ],
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
