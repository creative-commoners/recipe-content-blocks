{
    "name": "silverstripe/recipe-content-blocks",
    "type": "silverstripe-recipe",
    "description": "Add content blocks to your Silverstripe CMS project",
    "homepage": "https://www.silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1",
        "silverstripe/recipe-plugin": "~2.0.1@stable",
        "silverstripe/recipe-cms": "~5.3.0@beta",
        "dnadesign/silverstripe-elemental": "~5.3.0@beta",
        "silverstripe/elemental-fileblock": "~3.1.0@stable",
        "silverstripe/elemental-bannerblock": "~3.3.0@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "silverstripe/standards": "^1",
        "phpstan/extension-installer": "^1.3"
    },
    "suggest": {
        "dnadesign/silverstripe-elemental-userforms": "Add integration logic for Elemental and Userforms"
    },
    "extra": {
        "project-files": [
            "app/*"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}